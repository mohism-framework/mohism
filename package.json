{
  "name": "@mohism/mohism",
  "version": "1.2.12",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "echo \"\n\" && echo \" Run 'mohism --complete' enable completion. \"  && echo \"\n\"",
    "build": "npx tsc",
    "dev": "npx ts-node src/bin/index.ts",
    "lint": "npx eslint src/**/*.ts --cache --fix",
    "test": "npx mocha --recursive -r ts-node/register test/**/*.spec.ts",
    "start": "echo \"run 'sudo npm link' and 'mohism -h'\"",
    "cover": "npx nyc npm test"
  },
  "keywords": [
    "mohism",
    "moism"
  ],
  "author": "lanhao",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/colors": "^1.2.1",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.7.12",
    "@types/node": "^12.7.12",
    "@types/readline-sync": "^1.4.3",
    "@types/shelljs": "^0.8.5",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "husky": "^3.1.0",
    "mocha": "^8.1.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@mohism/core": "^0.4.2",
    "@mohism/prune-node-modules": "^1.0.2",
    "@mohism/publish": "^1.0.12",
    "@mohism/self-upgrade": "^1.0.4",
    "@mohism/sloty": "^1.0.3",
    "@mohism/ts-kit": "^1.1.4",
    "@mohism/utils": "^1.1.3",
    "inquirer": "^7.0.0",
    "readline-sync": "^1.4.10",
    "shelljs": "^0.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint & npm test"
    }
  },
  "bin": {
    "mohism": "dist/bin/index.js"
  },
  "mohismCmd": "mohism"
}
